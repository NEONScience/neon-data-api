{
  "name": "docs-app-graphiql",
  "version": "1.3.0",
  "private": true,
  "homepage": ".",
  "dependencies": {
    "@types/node": "^14.17.7",
    "@types/react": "^17.0.15",
    "@types/react-dom": "^17.0.9",
    "graphiql": "^1.4.2",
    "graphql": "^15.5.1",
    "isomorphic-fetch": "^3.0.0",
    "prop-types": "^15.7.2",
    "react": "~17.0.2",
    "react-dom": "~17.0.2",
    "react-scripts": "^4.0.3",
    "typescript": "~4.3.5"
  },
  "devDependencies": {
    "graphql-markdown": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^4.29.0",
    "@typescript-eslint/parser": "^4.29.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "typescript-tslint-plugin": "^1.0.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "generate:graphql-markdown": "yarn run graphql-markdown https://data.neonscience.org/graphql > ./graphql/schema.md",
    "lint": "(eslint src/ --ext .js,.jsx,.ts,.tsx || true) && exit 0",
    "lint:fix": "(eslint --fix src/ --ext .js,.jsx,.ts,.tsx || true) && exit 0"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
